{
	"name": "create-t3-turbo",
	"private": true,
	"engines": {
		"node": "^22.21.0",
		"pnpm": "^10.19.0"
	},
	"packageManager": "pnpm@10.19.0",
	"scripts": {
		"build": "turbo run build",
		"clean": "git clean -xdf node_modules",
		"clean:workspaces": "turbo run clean",
		"auth:generate": "pnpm -F @feather/auth generate",
		"db:push": "turbo -F @feather/db push",
		"db:studio": "turbo -F @feather/db studio",
		"dev": "turbo watch dev --continue",
		"dev:web": "turbo watch dev -F @feather/nextjs...",
		"lint": "turbo run lint",
		"format": "turbo run format",
		"check": "turbo run check",
		"check:fix": "turbo run check:fix",
		"lint:ws": "pnpm dlx sherif@latest",
		"lint:ws:fix": "pnpm dlx sherif@latest --fix",
		"postinstall": "pnpm lint:ws:fix",
		"typecheck": "turbo run typecheck",
		"ui-add": "turbo run ui-add",
		"android": "expo run:android",
		"ios": "expo run:ios"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@feather/prettier-config": "workspace:*",
		"@turbo/gen": "^2.5.8",
		"dotenv-cli": "^10.0.0",
		"prettier": "catalog:",
		"turbo": "^2.5.8",
		"typescript": "catalog:"
	},
	"prettier": "@feather/prettier-config"
}
